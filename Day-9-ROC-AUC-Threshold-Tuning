    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# ROC curve already computed: fpr, tpr, thresholds\n",
        "\n",
        "def mark_threshold(th):\n",
        "    idx = np.argmin(np.abs(thresholds - th))\n",
        "    plt.scatter(fpr[idx], tpr[idx], s=80, label=f\"threshold={th}\")\n",
        "\n",
        "plt.figure()\n",
        "plt.plot(fpr, tpr, label=f\"AUC = {auc_score:.2f}\")\n",
        "plt.plot([0,1], [0,1], linestyle='--')\n",
        "\n",
        "mark_threshold(0.5)\n",
        "mark_threshold(0.3)\n",
        "\n",
        "plt.xlabel(\"False Positive Rate\")\n",
        "plt.ylabel(\"True Positive Rate (Recall)\")\n",
        "plt.legend()\n",
        "plt.show()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 449
