        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# synthetic customer behaviour data\n",
        "data = pd.DataFrame({\n",
        "    \"hours_spent\": [1,2,2,8,9,10,1,7,8,2,9,6],\n",
        "    \"items_viewed\": [2,3,2,8,9,10,1,7,8,3,9,6],\n",
        "    \"income\": [60000,65000,62000,150000,160000,170000,\n",
        "               58000,145000,155000,63000,165000,140000]\n",
        "})\n",
        "\n",
        "X = data.values\n",
        "print(data)\n"
      ]
